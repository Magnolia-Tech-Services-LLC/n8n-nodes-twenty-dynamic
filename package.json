{
  "name": "@magnolia-tech/n8n-nodes-twenty-dynamic",
  "version": "0.10.1",
  "description": "n8n node for Twenty CRM with dynamic schema discovery and custom database support - Magnolia Tech fork",
  "keywords": [
    "n8n-community-node-package",
    "twenty",
    "crm",
    "magnolia-tech"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Magnolia-Tech-Services-LLC/n8n-nodes-twenty-dynamic",
  "author": {
    "name": "Magnolia Tech Services, LLC",
    "email": "hello@magnoliatechservices.com"
  },
  "contributors": [
    {
      "name": "Logrui",
      "email": "yhcsanction@gmail.com"
    },
    {
      "name": "s-yhc",
      "url": "https://github.com/s-yhc"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Magnolia-Tech-Services-LLC/n8n-nodes-twenty-dynamic.git"
  },
  "bugs": {
    "url": "https://github.com/Magnolia-Tech-Services-LLC/n8n-nodes-twenty-dynamic/issues"
  },
  "engines": {
    "node": ">=18.10"
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TwentyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Twenty/Twenty.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
